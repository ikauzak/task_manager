<h1>Tasks#index</h1>
<p>Find me in app/views/tasks/index.html.erb</p>
<% @tasks.each do |task| %>
  <div>
    <ol>
      <input disabled type="checkbox" <%= task.completed ? "checked" : "" %> >
      <%= link_to task.title, task_url(task) %>
      <%= link_to "Delete", task_path(task),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
    </ol>
  </div>
<% end %>
<%= link_to "+ Add new task", tasks_new_path %>
